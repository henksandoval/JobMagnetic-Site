{
  "name": "my-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en --port 5081",
    "start:es": "ng serve --configuration=es --port 5081",
    "build": "ng build --configuration=production",
    "watch": "ng build --watch --configuration=development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:autofix": "run-p lint:fix prettier:write",
    "prettier:write": "prettier --write \"**/*.{ts,html,js,json,md,yml,yaml}\"",
    "validate": "run-s lint test",
    "i18n": "ng extract-i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "~19.2.19",
    "@angular/common": "^20.1.6",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/material": "~19.2.19",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.7",
    "boxicons": "^2.1.4",
    "glightbox": "^3.3.1",
    "guid-typescript": "^1.0.9",
    "ngx-owl-carousel-o": "^19.0.2",
    "ngx-page-scroll": "^14.0.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zod": "^3.25.76",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/build": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.14",
    "@angular/localize": "^19.2.14",
    "@testing-library/angular": "^17.4.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/aos": "^3.0.7",
    "@types/jest": "^29.5.14",
    "angular-eslint": "19.8.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.32.0",
    "expect-type": "^1.2.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^14.6.1",
    "ng-mocks": "^14.13.5",
    "npm-run-all": "^4.1.5",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.39.0"
  }
}
